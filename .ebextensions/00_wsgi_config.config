option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: app:application
    # Remova a linha abaixo se você já configurou DEBUG_ANALISE via console EB
    # FLASK_APP: app.py
    # Remova a linha abaixo se você já configurou DEBUG_ANALISE via console EB
    # PYTHONPATH: /var/app/current
  aws:elasticbeanstalk:application:environment:
    # Essas variáveis já devem estar configuradas no console EB,
    # mas se preferir, pode adicioná-las aqui também.
    # TOGETHER_API_KEY: "SUA_CHAVE_AQUI"
    # DATABASE_URL: "SUA_URL_DO_DB_AQUI"
    # DEBUG_ANALISE: "True"
container_commands:
  01_migrate:
    command: "python app.py db upgrade" # Use app.py, não application.py.
    leader_only: true